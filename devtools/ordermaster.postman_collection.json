				{
					"listen": "test",
					"script": {
						"id": "8b93dcf8-f723-427d-8e18-31450fc106d5",
						"exec": [
							"var token1=postman.getResponseCookie('XSRF-TOKEN').value;",
							"var token2=token1.replace('%3D%3D','==');",
							"postman.setEnvironmentVariable('X-XSRF-TOKEN', token2);",
							""
						],
						"type": "text/javascript"
					}
